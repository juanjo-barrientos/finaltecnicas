<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>CvRoutesOne</servlet-name>
        <servlet-class>views.CvRoutesOne</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CvRoutesOne</servlet-name>
        <url-pattern>/api/cv/one</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CvRoutesTwo</servlet-name>
        <servlet-class>views.CvRoutesTwo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CvRoutesTwo</servlet-name>
        <url-pattern>/api/cv/two</url-pattern>
    </servlet-mapping>
</web-app>
